{
  "name": "sikomen-api-es6",
  "version": "1.0.0",
  "description": "Express.js API for sikomen-renuipsbt.com",
  "main": "index.js",
  "author": "shandy siswandi <shandysiswandi@gmail.com>",
  "developer": "dedy styawan <dedy.styawan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jadahbakar/sikomen-renuipsbt.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.5.0",
    "npm": ">=6.14.7"
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api"
  ],
  "scripts": {
    "app:build": "babel src --out-dir dist",
    "app:clean": "rimraf dist",
    "app:dev": "cross-env NODE_ENV=development npm-run-all app:lint app:clean app:build app:server",
    "app:lint": "standard --fix src/**/*.js",
    "app:server": "node ./dist/index.js",
    "test": "npm-run-all test:unit test:integration test:coverage",
    "test:coverage": "cross-env NODE_ENV=test nyc --require @babel/register --reporter text mocha --exit --recursive test",
    "test:integration": "cross-env NODE_ENV=test mocha --exit --recursive test/integration",
    "test:unit": "cross-env NODE_ENV=test mocha --exit --recursive test/unit",
    "watch": "nodemon"
  },
  "dependencies": {
    "archiver": "^5.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-rate-limit": "^5.1.3",
    "express-slow-down": "^1.3.1",
    "glob": "^7.1.6",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.11",
    "nodemailer-express-handlebars": "^4.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "redis": "^3.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-mail": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pm2": "^4.4.1",
    "supertest": "^4.0.2"
  }
}
